.banners {
  width: 100%;
  height: 300rpx;
}
.banners image {
  width: 100%;
  height: 100%;
}
.navContainer {
  display: flex;
}
.navItem {
  display: flex;
  flex-direction: column;
  width: 20%;
  align-items: center;
}
.navItem .iconfont {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  font-size: 50rpx;
  color: #fff;
  background-color: rgb(240, 19, 19);
  margin: 20rpx 0;
}
.navItem text{
  font-size: 26rpx;
}
